{
	"name": "soundify",
	"version": "1.0.0",
	"description": "Music App",
	"main": "src/app.js",
	"scripts": {
		"start": "yarn build && yarn server:prod",
		"dev": "cross-env NODE_ENV=development webpack-dev-server",
		"build": "cross-env NODE_ENV=production webpack",
		"watch": "cross-env NODE_ENV=production webpack -w",
		"server": "nodemon --watch server --exec babel-node -- server/index.js",
		"server:prod": "cross-env NODE_ENV=production babel-node server",
		"test": "jest --watch",
		"lint:js": "eslint ./src/**/*.js --fix",
		"lint:css": "stylelint ./src/**/*.scss --fix"
	},
	"jest": {
		"setupTestFrameworkScriptFile": "<rootDir>/test.config.js",
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/styleMock.js",
			"\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
		}
	},
	"author": "Ilya Pashkov",
	"license": "ISC",
	"proxy": "http://localhost:8080",
	"devDependencies": {
		"autoprefixer": "^8.3.0",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.0.2",
		"babel-loader": "^7.1.2",
		"babel-plugin-module-resolver": "^3.1.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"clean-webpack-plugin": "^0.1.17",
		"cross-env": "^5.1.3",
		"css-loader": "^0.28.7",
		"dotenv": "^5.0.1",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.3",
		"eslint": "^4.11.0",
		"eslint-config-prettier": "^2.7.0",
		"eslint-loader": "^2.0.0",
		"eslint-plugin-prettier": "^2.3.1",
		"eslint-plugin-react": "^7.5.0",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"html-webpack-plugin": "^3.0.6",
		"ignore-styles": "^5.0.1",
		"jest": "^22.4.3",
		"json-loader": "^0.5.7",
		"node-sass": "^4.7.2",
		"postcss-loader": "^2.1.4",
